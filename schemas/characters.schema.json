{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "array",
    "title": "The Root Schema",
    "items": {
      "$id": "#/items",
      "type": "object",
      "title": "The Items Schema",
      "required": [
        "id",
        "url"
      ],
      "properties": {
        "id": {
          "$id": "#/items/properties/id",
          "type": "string",
          "title": "The Id Schema",
          "default": "",
          "examples": [
            "megumi"
          ],
          "pattern": "^(.*)$"
        },
        "url": {
          "$id": "#/items/properties/url",
          "type": "string",
          "title": "The Url Schema",
          "default": "",
          "examples": [
            "https://www.saenai.tv/images/character/chara_megumi_vsl.png"
          ],
          "pattern": "^(.*)$"
        }
      }
    }
  }